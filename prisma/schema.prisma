//客户端
generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
}

//数据库
datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id       Int     @id @default(autoincrement()) @db.UnsignedInt
  name     String  @unique
  password String
  email    String? @unique
}

model article {
  id      Int    @id @default(autoincrement()) @db.UnsignedInt
  title   String
  content String @db.Text
}
